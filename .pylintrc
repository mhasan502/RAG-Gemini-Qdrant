[MASTER]
py-version=3.10.13

[FORMAT]
max-line-length=120
indent-string='    '


[STRING]
check-quote-consistency=yes


[REPORTS]
output-format=colorized
logging-modules=logging


[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
	import-error,
	no-else-raise,
	broad-except,
    global-statement